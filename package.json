{
  "name": "nicofinder-extension",
  "description": "Nicofinder WebExtension",
  "version": "3.0.0",
  "author": "Hayu <admin@hayu.io>",
  "homepage": "http://www.nicofinder.net/extension",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/githayu/nicofinder-extension.git"
  },
  "scripts": {
    "start": "npm-run-all clean -p start:*",
    "start:gulp": "gulp",
    "start:webpack": "webpack --watch --progress --config ./config/webpack.dev.js",
    "build": "run-s clean build:*",
    "build:gulp": "gulp build --env production",
    "build:webpack": "webpack -p --config ./config/webpack.prod.js",
    "build:release": "cd ./dist && zip -qr ../nicofinder-extension.zip ./",
    "compile": "gulp build && webpack",
    "clean": "rm -rf *.zip ./dist",
    "test": "jest"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-helper-regex": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-merge-json": "^1.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.8",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.16.0"
  }
}
