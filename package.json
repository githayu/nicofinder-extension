{
  "name": "nicofinder-extension",
  "description": "Nicofinder WebExtension",
  "version": "2.0.0",
  "author": "Hayu <admin@hayu.io>",
  "homepage": "https://chrome.google.com/webstore/detail/nicofinder/jgnhfelllimcnjaoofphfjiepgfkdbed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/githayu/nicofinder-extension.git"
  },
  "scripts": {
    "start": "run-p start:*",
    "start:gulp": "gulp",
    "start:webpack": "webpack -d --watch --progress --config config/webpack.dev.js",
    "build": "run-s build:*",
    "build:gulp": "gulp build --env production",
    "build:webpack": "webpack --progress --config config/webpack.prod.js",
    "build:release": "cd ./dist && zip -qr ../nicofinder-extension.zip ./",
    "compile": "gulp build && webpack"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-react": "^7.0.0-beta.35",
    "@babel/preset-stage-1": "^7.0.0-beta.35",
    "autoprefixer": "^7.2.3",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.1",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-jsonminify": "^1.0.0",
    "gulp-merge-json": "^1.0.0",
    "gulp-plumber": "^1.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-merge": "^4.1.1"
  }
}
