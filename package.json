{
  "name": "nicofinder-extension",
  "description": "NicofinderExtension",
  "version": "2.0.0",
  "author": "Hayu <admin@nanoway.net>",
  "homepage": "http://www.nicofinder.net/extension",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/githayu/nicofinder-extension.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:gulp": "gulp",
    "start:webpack": "webpack --watch --progress",
    "build": "npm-run-all -s build:clean -p build:gulp build:webpack -s build:release",
    "build:clean": "rm -Rf ./dist",
    "build:gulp": "gulp all --production",
    "build:webpack": "webpack -p",
    "build:release": "cd ./dist && zip -qr ../nicofinder-extension.zip ./",
    "compile": "gulp all && webpack"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-env": "0.0.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "extract-text-webpack-plugin": "webpack/extract-text-webpack-plugin#v2.0.0-beta.4",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-if": "^2.0.1",
    "gulp-ignore": "^2.0.1",
    "gulp-jsonminify": "^1.0.0",
    "gulp-merge-json": "^0.6.0",
    "gulp-plumber": "^1.1.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.0",
    "lodash.get": "^4.4.2",
    "minimist": "^1.2.0",
    "npm-run-all": "^3.1.0",
    "postcss-loader": "^1.1.1",
    "precss": "^1.4.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "style-loader": "^0.13.1",
    "webpack": "2.1.0-beta.27"
  }
}
