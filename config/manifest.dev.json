{
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*.nicofinder.net/*"
      ],
      "js": [
        "js/vendor.js",
        "js/content_scripts/extension-provider.js",
        "js/content_scripts/nicofinder.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
